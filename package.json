{
  "name": "@wutiskill/branding",
  "version": "1.0.0",
  "description": "Multi-tenant branding for WutiSkill LMS platform",
  "author": "WutiSkill",
  "license": "UNLICENSED",
  "scripts": {
    "build-tokens:wutiskill": "paragon build-tokens --source ./branding/wutiskill/tokens/src --build-dir ./branding/wutiskill/paragon/css",
    "build-scss:wutiskill": "paragon build-scss --corePath ./branding/wutiskill/paragon/core.scss --themesPath ./branding/wutiskill/paragon/css/themes --outDir ./dist/wutiskill --defaultThemeVariants light",
    "build:wutiskill": "npm run build-tokens:wutiskill && npm run build-scss:wutiskill",
    "build-tokens:10000codeurs": "paragon build-tokens --source ./branding/10000codeurs/tokens/src --build-dir ./branding/10000codeurs/paragon/css",
    "build-scss:10000codeurs": "paragon build-scss --corePath ./branding/10000codeurs/paragon/core.scss --themesPath ./branding/10000codeurs/paragon/css/themes --outDir ./dist/10000codeurs --defaultThemeVariants light",
    "build:10000codeurs": "npm run build-tokens:10000codeurs && npm run build-scss:10000codeurs",
    "build": "node ./scripts/build-all.js",
    "paragon:help": "paragon help"
  },
  "devDependencies": {
    "@openedx/paragon": "^23.18.2"
  },
  "dependencies": {
    "@edx/edx-bootstrap": "^2.6.0"
  }
}